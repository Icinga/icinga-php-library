{
    "name": "icinga/icinga-php-library",
    "type": "project",
    "description": "Icinga Web 2 - bundled Icinga PHP libraries",
    "homepage": "https://github.com/Icinga/icinga-php-library",
    "license": "MIT",
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "8.2"
        },
        "allow-plugins": {
            "cweagans/composer-patches": true
        }
    },
    "require": {
        "php": ">=8.2",
        "ipl/html": "^0.8.0",
        "ipl/i18n": "^0.2.0",
        "ipl/orm": "^0.6.0",
        "ipl/scheduler": "^0.1.0",
        "ipl/sql": "^0.7.0",
        "ipl/stdlib": "^0.14.0",
        "ipl/validator": "^0.5.0",
        "ipl/web": "^0.10.0",
        "cweagans/composer-patches": "~1.0"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-0": { "AssetLoader": "" }
    },
    "scripts": {
        "post-update-cmd": [
            "AssetLoader::update"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patches": {
            "ramsey/collection": {
                "Collection: Add PHP 8.1 support": "patches/ramsey-collection.patch"
            }
        }
    }
}
