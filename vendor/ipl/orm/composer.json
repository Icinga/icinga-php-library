{
  "name": "ipl/orm",
  "type": "library",
  "description": "Icinga PHP Library - ORM",
  "license": "MIT",
  "keywords": [
    "sql",
    "database",
    "orm"
  ],
  "homepage": "https://github.com/Icinga/ipl-orm",
  "require": {
    "php": ">=7.2",
    "ext-pdo": "*",
    "ipl/sql": ">=0.6.0",
    "ipl/stdlib": ">=0.12.0"
  },
  "autoload": {
    "psr-4": {
      "ipl\\Orm\\": "src"
    }
  },
  "require-dev": {
    "ext-pdo_sqlite": "*"
  },
  "autoload-dev": {
    "psr-4": {
      "ipl\\Tests\\Orm\\": "tests",
      "ipl\\Tests\\Sql\\": "vendor/ipl/sql/tests"
    }
  }
}
